{#
/**
 * @file
 * Theme override to display primary and secondary local tasks.
 *
 * Available variables:
 * - primary: HTML list items representing primary tasks.
 * - secondary: HTML list items representing primary tasks.
 *
 * Each item in these variables (primary and secondary) can be individually
 * themed in menu-local-task.html.twig.
 */
#}
{% if primary %}
  <nav role="navigation" aria-labelledby="primary-tabs-heading">
    <h2 class="visually-hidden" id="primary-tabs-heading">{{ 'Primary tabs'|t }}</h2>
    <ul>{{ primary }}</ul>
  </nav>
{% endif %}
{% if secondary %}
  <nav role="navigation" aria-labelledby="secondary-tabs-heading">
    <h2 class="visually-hidden" id="secondary-tabs-heading">{{ 'Secondary tabs'|t }}</h2>
    <ul>{{ secondary }}</ul>
  </nav>
{% endif %}
